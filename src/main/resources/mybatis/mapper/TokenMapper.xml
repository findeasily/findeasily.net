<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.findeasily.website.mapper.TokenMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.findeasily.website.domain.Token">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="val" property="val"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <delete id="deleteFromHourBefore">
        delete from token where create_time &lt; DATE_SUB(NOW() , INTERVAL #{hour} HOUR );
    </delete>

</mapper>
